{
  "name": "winst-backend",
  "version": "1.0.0",
  "description": "Backend API for Winst Internship Portal",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "start:development": "cross-env NODE_ENV=development node server.js",
    "start:production": "cross-env NODE_ENV=production node server.js",
    "dev:development": "cross-env NODE_ENV=development nodemon server.js",
    "dev:production": "cross-env NODE_ENV=production nodemon server.js",
    "smtp:test": "node scripts/test-smtp.js",
    "pm2:start": "pm2 start pm2.config.cjs",
    "pm2:start:dev": "pm2 start pm2.config.cjs --env development",
    "pm2:start:prod": "pm2 start pm2.config.cjs --env production",
    "pm2:stop": "pm2 stop pm2.config.cjs",
    "pm2:restart": "pm2 restart pm2.config.cjs",
    "pm2:delete": "pm2 delete pm2.config.cjs",
    "pm2:status": "pm2 status",
    "pm2:logs": "pm2 logs winst-backend",
    "pm2:monit": "pm2 monit",
    "health": "curl -f http://localhost:3001/health || exit 1",
    "logs": "type logs\\app.log",
    "clean": "rmdir /s /q node_modules && del package-lock.json && npm install",
    "postinstall": "echo '\\n✅ Winst Backend installed successfully!\\n� Run npm run dev to start development server\\n� See package.json for all available commands\\n'"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "pdfkit": "^0.13.0",
    "pg": "^8.16.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "internship",
    "api",
    "nodejs",
    "postgresql"
  ],
  "author": "Winst Team",
  "license": "MIT"
}
