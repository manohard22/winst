{
  "name": "winst-backend",
  "version": "1.0.0",
  "description": "Backend API for Winst Internship Portal",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "start:local": "pm2 start pm2.config.cjs --env development",
    "restart:local": "pm2 restart pm2.config.cjs --env development",
    "stop:local": "pm2 delete pm2.config.cjs",
    "build:server": "node -e \"console.log('Backend has no build step')\"",
    "start:server": "pm2 start pm2.config.cjs --env development",
    "restart:server": "pm2 restart pm2.config.cjs --env development",
    "stop:server": "pm2 delete pm2.config.cjs",
    "pm2:start": "pm2 start pm2.config.cjs",
    "pm2:stop": "pm2 stop pm2.config.cjs",
    "pm2:restart": "pm2 restart pm2.config.cjs",
    "pm2:delete": "pm2 delete pm2.config.cjs",
    "pm2:status": "pm2 status",
    "pm2:logs": "pm2 logs winst-backend"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "nodemailer": "^6.9.7",
    "pdfkit": "^0.13.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0"
  },
  "keywords": [
    "internship",
    "api",
    "nodejs",
    "postgresql"
  ],
  "author": "Winst Team",
  "license": "MIT"
}
