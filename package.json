{
  "name": "lucro-internship-portal",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "npm --prefix server run dev",
    "build:client": "vite build",
    "build:server": "npm --prefix server run build",
    "build": "npm run build:client && npm run build:server",
    "start:client": "vite preview",
    "start:server": "npm --prefix server run start",
    "start": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "staging:client": "vite build && vite preview",
    "staging:server": "npm --prefix server run staging",
    "staging": "concurrently \"npm run staging:client\" \"npm run staging:server\""
  },
  "dependencies": {
    "axios": "^1.3.4",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.8.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "vite": "^4.2.0"
  }
}
